{
	"extends": "astro/tsconfigs/strictest",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": [
				"src/components/*"
			],
			"@layouts/*": [
				"src/layouts/*"
			],
			"@assets/*": [
				"src/assets/*"
			]
		},
		"types": [
			"@types/dom-view-transitions",
			"astro/client",
			"bun-types"
		]
	}
}